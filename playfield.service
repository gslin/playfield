#
[Unit]
Description=Description

[Service]
ExecStart=/usr/bin/env mise exec uv@latest -- uv run gunicorn --bind unix:/home/service/tmp/playfield.sock --error-logfile /tmp/playfield-gunicorn-error.log --preload --umask 0000 app:app app.py
Restart=on-failure
RestartSec=60
Type=simple
WorkingDirectory=/home/service/playfield

[Install]
WantedBy=default.target
